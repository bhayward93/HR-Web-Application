@model IEnumerable<SkillModel.Skill>

@{
    ViewBag.Title = "Skills";
}

<h2>Skills</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StaffCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SkillCode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StaffCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SkillCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StaffSkillsId }) |
            @Html.ActionLink("Details", "Details", new { id=item.StaffSkillsId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StaffSkillsId })
        </td>
    </tr>
}

</table>
